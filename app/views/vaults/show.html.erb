<%= component "add" %>
<%= component "heading" do %>
  <%= link_to edit_vault_path(@vault), class: "" do %>
    <h1><%= @vault.name %></h1>
  <% end %>
<% end %>
<% if @vault.links.count != 0 %>
  <%= render @vault.links.order(created_at: :desc) %>
<% else %>
  <%= component "null" %>
<% end %>
<%= link_to "Delete", @vault, method: :delete, data: { confirm: 'Are you sure?' }, class: "" %>
